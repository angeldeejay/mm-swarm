    "js-yaml": "^4.1.0",
    "npm": "^8.18.0",
    "parse-git-config": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "prettier": "latest"
